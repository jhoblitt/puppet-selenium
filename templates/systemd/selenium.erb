# <%= @prog %> Selenium server init script
[Unit]
Description=Selenium Server (<%= @prog %>)
After=network.target network-online.target

[Service]
EnvironmentFile=/etc/<%= @prog %>.conf
ExecStart=/usr/bin/java -jar <%= @install_root %>/jars/<%= @jar_name %> $SELENIUM_SERVER_ARGS $SELENIUM_SERVER_LOG_ARGS
KillMode=process
User=<%= @user %>

[Install]
WantedBy=multi-user.target